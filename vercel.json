{
  "version": 2,
  "builds": [
    { "src": "*.txt", "use": "@vercel/static" },
    { "src": "server/index.js", "use": "@vercel/node" },
    { "src": "server", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/index.js"
    }
  ],
  "staticAssets": [
    {
      "src": "dist/*.css",
      "headers": {
        "Content-Type": "text/css"
      }
    }
  ]
}
